package ie.ucd.onetwotwofive.practical4;


/*  CompCourse
 * 	This Class represents a course in the CSI set of courses
 * 
 */
public class CompCourse {

	public String name;
	public int credits;
	public String description;
	
	CompCourse(String newName, int newInt, String newDesc) {
		//Constructor 
		name = newName;
		credits = newInt;
		description = newDesc;
	}
}
